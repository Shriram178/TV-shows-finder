  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="HtmlAgilityPack" Version="1.12.0" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\search.png" />
    <Resource Include="Images\star.png" />
    <Resource Include="Images\status.png" />
